{
  "name": "wavevision/namespace-translator",
  "description": "Manage Nette app translations on per namespace basis.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "contributte/translation": "^0.6.0",
    "nette/application": "^3.0",
    "nette/di": "^3.0",
    "nette/neon": "^3.0",
    "symfony/console": "^4.3",
    "wavevision/utils": "^2.1"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^1.0",
    "phing/phing": "3.0.0-alpha2",
    "php-coveralls/php-coveralls": "^2.2",
    "php-mock/php-mock-phpunit": "^2.5",
    "phpstan/phpstan": "^0.12.3",
    "phpunit/phpunit": "^8.4",
    "wavevision/coding-standard": "^2.1",
    "wavevision/nette-tests": "^2.2"
  },
  "scripts": {
    "phing": "phing"
  },
  "extra": {
    "phpstan": {}
  }
}
